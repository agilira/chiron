{
  "source": {
    "include": ["builder"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|tests/)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs-api",
    "recurse": true,
    "readme": "./README.md"
  }
}
